	collisionCheck();
	
	if (frog_grounded){ // Maassa
		frog_vy = 0.0;
		frog_ay = 0.0;
		jumpTimer = 0.10;
	}
	else {
		frog_ay = -40.0; // Ilmassa
		jumpTimer = jumpTimer -dt;
	}
	if (jumpTimer < 0){
		jumpTimer = 0;
	}
	setCamera([frog_x, frog_y], 0.2);
	      
____drawTexture("background",[frog_x/1.1, frog_y/1.1], [5.0, 5.0], 0.0, [1.0, 1.0, 1.0, 0.9]);